<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <suppress>
        <notes>Suppressing lodash.template vulnerability (CVE ID)</notes>
        <packageUrl regex="true">^pkg:npm/lodash\.template@.*$</packageUrl>
        <cve>CVE-2021-23337</cve>
    </suppress>
    <suppress>
        <notes>Suppressing lodash.template vulnerability (GHSA ID)</notes>
        <packageUrl regex="true">^pkg:npm/lodash\.template@.*$</packageUrl>
        <vulnerabilityName>GHSA-35jh-r3h4-6jhm</vulnerabilityName>
    </suppress>
    <suppress>
        <notes>Suppressing Terraform provider vulnerabilities</notes>
        <filePath regex="true">.*\.(tf|tfstate|tfvars|exe)$</filePath>
        <cve>CVE-2021-32690</cve>
    </suppress>
</suppressions> 